{
  "manifest_version": 2,
  "name": "Domain-based QBench Navbar Color",
  "version": "1.0",
  "description": "Change QBench navbar color based on domain",
  "permissions": [
    "activeTab",
    "tabs",
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["colorNavbar.js"],
      "run_at": "document_idle"
    }
  ]
}
